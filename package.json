{
  "name": "zombordle",
  "version": "0.1.0",
  "private": true,
  "workspaces": [
    "app",
    "packages/*"
  ],
  "scripts": {
    "dev": "yarn workspace @app/zombordle dev",
    "export": "yarn turbo run export --cache-dir='./.cache'",
    "start": "yarn workspace @app/zombordle start",
    "build": "yarn turbo run build --cache-dir='./.cache'",
    "lint": "yarn turbo run lint --color --cache-dir='./.cache'",
    "test": "yarn turbo run test --color --cache-dir='./.cache'",
    "lint-css": "stylelint ./**/*.{ts,tsx}",
    "test-related": "yarn workspaces foreach run test-related",
    "postinstall": "husky install"
  },
  "devDependencies": {
    "@svgr/webpack": "^8.0.1",
    "@testing-library/dom": "^9.0.1",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@tophat/eslint-config": "^7.0.1",
    "@tophat/eslint-import-resolver-require": "^0.1.4",
    "@types/node": "^20.2.5",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.2.4",
    "@types/styled-components": "^5.1.26",
    "@types/testing-library__jest-dom": "^5.14.5",
    "@typescript-eslint/eslint-plugin": "^5.59.1",
    "@typescript-eslint/parser": "^5.59.0",
    "@vitejs/plugin-react": "^3.1.0",
    "@yarnpkg/sdks": "^3.0.0-rc.42",
    "babel-plugin-styled-components": "^2.1.1",
    "eslint": "^8.41.0",
    "eslint-config-next": "^13.4.4",
    "eslint-config-prettier": "^8.8.0",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jest-dom": "^4.0.3",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-testing-library": "^5.10.2",
    "hoist-non-react-statics": "^3.3.2",
    "husky": "^8.0.0",
    "jsdom": "^21.1.0",
    "lint-staged": "^13.2.1",
    "postcss": "^8.4.21",
    "postcss-styled": "^0.34.0",
    "postcss-styled-syntax": "^0.4.0",
    "postcss-syntax": "^0.36.2",
    "prettier": "^2.8.7",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-is": "^18.2.0",
    "styled-components": "^5.3.8",
    "stylelint": "^15.6.2",
    "stylelint-config-standard": "^30.0.1",
    "supports-color": "^9.3.1",
    "turbo": "^1.9.3",
    "typescript": "^5.0.3",
    "vite": "^4.1.4",
    "vitest": "^0.30.1"
  },
  "resolutions": {
    "@testing-library/dom": "^9.0.1"
  },
  "packageManager": "yarn@3.5.0"
}
